-- WalkSpeed, JumpPower, Invincibility, Anti-Stun, and Noclip Hack with GUI (Persistent After Death)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local StarterGui = game:GetService("StarterGui")

local invincible = true
local noclipEnabled = false
local currentSpeed = 16
local currentJump = 50

local function applyStats(Humanoid)
    Humanoid.WalkSpeed = currentSpeed
    Humanoid.JumpPower = currentJump
    if invincible then
        Humanoid:ChangeState(11)
        Humanoid.PlatformStand = false
        if Humanoid.Health < Humanoid.MaxHealth then
            Humanoid.Health = Humanoid.MaxHealth
        end
    end
end

local function applyNoclip(Character)
    if noclipEnabled then
        for _, part in ipairs(Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end

local function setupGUI()
    local screenGui = Instance.new("ScreenGui")
    screenGui.Name = "SpeedJumpGUI"
    screenGui.ResetOnSpawn = false
    screenGui.Parent = game:GetService("CoreGui")

    local frame = Instance.new("Frame")
    frame.Size = UDim2.new(0, 250, 0, 190)
    frame.Position = UDim2.new(0, 100, 0, 100)
    frame.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
    frame.BorderSizePixel = 0
    frame.Active = true
    frame.Draggable = true
    frame.Parent = screenGui

    local labelSpeed = Instance.new("TextLabel")
    labelSpeed.Size = UDim2.new(1, 0, 0, 25)
    labelSpeed.Position = UDim2.new(0, 0, 0, 0)
    labelSpeed.BackgroundTransparency = 1
    labelSpeed.Text = "WalkSpeed: " .. currentSpeed
    labelSpeed.TextColor3 = Color3.new(1, 1, 1)
    labelSpeed.Font = Enum.Font.SourceSansBold
    labelSpeed.TextSize = 20
    labelSpeed.Parent = frame

    local speedButton = Instance.new("TextButton")
    speedButton.Size = UDim2.new(1, -20, 0, 25)
    speedButton.Position = UDim2.new(0, 10, 0, 25)
    speedButton.BackgroundColor3 = Color3.fromRGB(100, 0, 0)
    speedButton.TextColor3 = Color3.new(1, 1, 1)
    speedButton.Font = Enum.Font.SourceSans
    speedButton.TextSize = 16
    speedButton.Text = "Set Speed"
    speedButton.Parent = frame

    local speedInput = Instance.new("TextBox")
    speedInput.Size = UDim2.new(1, -20, 0, 25)
    speedInput.Position = UDim2.new(0, 10, 0, 50)
    speedInput.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    speedInput.TextColor3 = Color3.new(1, 1, 1)
    speedInput.Font = Enum.Font.SourceSans
    speedInput.TextSize = 16
    speedInput.Text = tostring(currentSpeed)
    speedInput.Parent = frame

    local labelJump = Instance.new("TextLabel")
    labelJump.Size = UDim2.new(1, 0, 0, 25)
    labelJump.Position = UDim2.new(0, 0, 0, 75)
    labelJump.BackgroundTransparency = 1
    labelJump.Text = "JumpPower: " .. currentJump
    labelJump.TextColor3 = Color3.new(1, 1, 1)
    labelJump.Font = Enum.Font.SourceSansBold
    labelJump.TextSize = 20
    labelJump.Parent = frame

    local jumpButton = Instance.new("TextButton")
    jumpButton.Size = UDim2.new(1, -20, 0, 25)
    jumpButton.Position = UDim2.new(0, 10, 0, 100)
    jumpButton.BackgroundColor3 = Color3.fromRGB(100, 0, 0)
    jumpButton.TextColor3 = Color3.new(1, 1, 1)
    jumpButton.Font = Enum.Font.SourceSans
    jumpButton.TextSize = 16
    jumpButton.Text = "Set Jump"
    jumpButton.Parent = frame

    local jumpInput = Instance.new("TextBox")
    jumpInput.Size = UDim2.new(1, -20, 0, 25)
    jumpInput.Position = UDim2.new(0, 10, 0, 125)
    jumpInput.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
    jumpInput.TextColor3 = Color3.new(1, 1, 1)
    jumpInput.Font = Enum.Font.SourceSans
    jumpInput.TextSize = 16
    jumpInput.Text = tostring(currentJump)
    jumpInput.Parent = frame

    local invincBtn = Instance.new("TextButton")
    invincBtn.Size = UDim2.new(1, -20, 0, 20)
    invincBtn.Position = UDim2.new(0, 10, 0, 150)
    invincBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    invincBtn.TextColor3 = Color3.new(1, 1, 1)
    invincBtn.Font = Enum.Font.SourceSansBold
    invincBtn.TextSize = 14
    invincBtn.Text = "Invincibility: " .. (invincible and "ON" or "OFF")
    invincBtn.Parent = frame

    local noclipBtn = Instance.new("TextButton")
    noclipBtn.Size = UDim2.new(1, -20, 0, 20)
    noclipBtn.Position = UDim2.new(0, 10, 0, 170)
    noclipBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
    noclipBtn.TextColor3 = Color3.new(1, 1, 1)
    noclipBtn.Font = Enum.Font.SourceSansBold
    noclipBtn.TextSize = 14
    noclipBtn.Text = "Noclip: " .. (noclipEnabled and "ON" or "OFF")
    noclipBtn.Parent = frame

    speedButton.MouseButton1Click:Connect(function()
        local newSpeed = tonumber(speedInput.Text)
        if newSpeed and newSpeed >= 0 and newSpeed <= 1000 then
            currentSpeed = newSpeed
            labelSpeed.Text = "WalkSpeed: " .. newSpeed
        else
            StarterGui:SetCore("SendNotification", {
                Title = "Speed Hack",
                Text = "Invalid speed (0-1000 only)",
                Duration = 2
            })
        end
    end)

    jumpButton.MouseButton1Click:Connect(function()
        local newJump = tonumber(jumpInput.Text)
        if newJump and newJump >= 0 and newJump <= 1000 then
            currentJump = newJump
            labelJump.Text = "JumpPower: " .. newJump
        else
            StarterGui:SetCore("SendNotification", {
                Title = "Jump Hack",
                Text = "Invalid jump power (0-1000 only)",
                Duration = 2
            })
        end
    end)

    invincBtn.MouseButton1Click:Connect(function()
        invincible = not invincible
        invincBtn.Text = "Invincibility: " .. (invincible and "ON" or "OFF")
    end)

    noclipBtn.MouseButton1Click:Connect(function()
        noclipEnabled = not noclipEnabled
        noclipBtn.Text = "Noclip: " .. (noclipEnabled and "ON" or "OFF")
    end)
end

setupGUI()

local function onCharacterAdded(char)
    Character = char
    Humanoid = Character:WaitForChild("Humanoid")
    Humanoid.Died:Connect(function()
        Character = nil
        Humanoid = nil
    end)
end

if LocalPlayer.Character then
    onCharacterAdded(LocalPlayer.Character)
end

LocalPlayer.CharacterAdded:Connect(onCharacterAdded)

RunService.RenderStepped:Connect(function()
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        applyStats(LocalPlayer.Character.Humanoid)
        applyNoclip(LocalPlayer.Character)
    end
end)
